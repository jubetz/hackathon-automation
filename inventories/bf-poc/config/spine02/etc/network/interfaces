# Ansible Managed File
auto lo
iface lo inet loopback
    address 10.0.1.102/32
auto mgmt
iface mgmt
    vrf-table auto
    address 127.0.0.1/8
    address ::1/128


auto eth0
iface eth0 inet dhcp
    vrf mgmt
auto swp1
iface swp1
    alias leaf to spine
 
auto swp2
iface swp2
    alias leaf to spine
 
auto swp3
iface swp3
    alias leaf to spine
 
